  "scripts": {
    "prebuild": "del dist src/manifest.json",
    "build": "run-p build:*",
    "build:app": "wait-on src/manifest.json && tsc --project tsconfig.build.json",
    "build:assets": "webpack build --mode production",
    "build:translations": "mkdir -p ./src/translations && docker build -f translationDockerfile -t intlc . && docker run -v ./locales-icu:/locales intlc compile /locales/en.json -l en > ./src/translations/en.ts && docker run -v ./locales-icu:/locales intlc compile /locales/es.json -l es > ./src/translations/es.ts",
    "format": "prettier --ignore-unknown --check '**'",
    "format:fix": "npm run format -- --write",
    "lint": "run-p --aggregate-output lint:*",
    "lint:css": "stylelint '**/*.css'",
    "lint:ts": "eslint . --max-warnings 0",
    "start": "run-p start:*",
    "start:app": "wait-on src/manifest.json && ts-node-dev --require dotenv/config --watch .env --transpile-only src/index.ts",
    "start:assets": "webpack watch --mode development",
    "start:translations": "nodemon --watch locales-icu --exec \"npm run build:translations\"",
    "test": "jest",
    "test:integration": "docker run --rm --volume \"$(pwd)\"/integration/snapshots:/app/integration/snapshots --volume \"$(pwd)\"/integration-results:/app/integration-results",
    "typecheck": "tsc --noEmit"
  }
