  "scripts": {
    "prebuild": "del dist src/manifest.json",
    "build": "run-p build:*",
    "build:app": "wait-on src/manifest.json && tsc --project tsconfig.build.json",
    "build:assets": "webpack build --mode production",
    "format": "prettier --ignore-unknown --check '**'",
    "format:fix": "npm run format -- --write",
    "lint": "run-p --aggregate-output lint:*",
    "lint:css": "stylelint '**/*.css'",
    "lint:ts": "eslint . --max-warnings 0",
    "start": "run-p start:*",
    "start:app": "wait-on src/manifest.json && ts-node-dev --require dotenv/config --transpile-only src/index.ts",
    "start:assets": "webpack watch --mode development",
    "test": "jest",
    "test:integration": "docker run --rm --volume \"$(pwd)\"/integration/snapshots:/app/integration/snapshots --volume \"$(pwd)\"/integration-results:/app/integration-results",
    "typecheck": "tsc --noEmit"
  }
